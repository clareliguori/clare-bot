{
  "executionRoleArn": "arn:aws:iam::131296546870:role/clare-bot-TaskExecutionRole-1SA47VA62G9D",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "clare-bot",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "bot-logs"
        }
      },
      "environment": [
        {
          "name": "botUser",
          "value": "clare-bot"
        },
        {
          "name": "whitelistedUsers",
          "value": "clareliguori"
        }
      ],
      "secrets": [
        {
          "valueFrom": "clare-bot-github-token",
          "name": "githubToken"
        }
      ],
      "image": "131296546870.dkr.ecr.us-west-2.amazonaws.com/clare-bot:latest",
      "essential": true,
      "name": "bot"
    }
  ],
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::131296546870:role/clare-bot-TaskRole-10KJX064T8LJZ",
  "family": "clare-bot",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256"
}
